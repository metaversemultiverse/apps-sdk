{
  "name": "@mondaydotcomorg/apps-sdk",
  "version": "1.0.0",
  "description": "monday apps SDK for NodeJS",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest --forceExit",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint --ext .js,.ts lib",
    "lint-fix": "npm run lint -- --fix",
    "prettier:fix": "prettier --write .",
    "prettier:check": "prettier --check .",
    "build": "tsc --project ./tsconfig.json && tsc-alias -f -p tsconfig.json",
    "prepublish-and-build": "yarn install --frozen-lockfile && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mondaycom/apps-sdk.git"
  },
  "keywords": [
    "MONDAY",
    "APPS",
    "SDK",
    "MONDAY-CODE"
  ],
  "engines": {
    "node": ">= 14.17.0"
  },
  "author": "monday.com Apps Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mondaycom/apps-sdk/issues"
  },
  "homepage": "https://github.com/mondaycom/apps-sdk#readme",
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/jsonwebtoken": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "tsc-alias": "^1.8.4",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "google-auth-library": "^8.7.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "lowdb": "^5.1.0",
    "node-fetch": "^3.3.1"
  }
}
